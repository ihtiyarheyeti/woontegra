# === Database ===
DB_HOST=localhost
DB_USER=root
DB_PASS=
DB_NAME=entegrasyon-paneli
DB_PORT=3306

# === Server ===
PORT=3001
NODE_ENV=development

# === Authentication ===
JWT_SECRET=supersecretkey
AUTH_REQUIRED=false

# === Public Routes (comma-separated) ===
PUBLIC_ROUTES=/health

# === Trendyol API ===
TRENDYOL_SUPPLIER_ID=113278
TRENDYOL_APP_KEY=CK_XXX
TRENDYOL_APP_SECRET=CS_XXX

# === Trendyol Fetch Mode ===
# "local" = Direct API call from local server
# "remote" = Proxy through production server
TRENDYOL_FETCH_MODE=local
TRENDYOL_REMOTE_BASE=https://woontegra.com

# === Cache Settings ===
CATEGORY_CACHE_DURATION_MS=3600000

# === Cloudflare Bypass ===
CF_MAX_RETRIES=3
CF_BACKOFF_MS=1000
OUTBOUND_PROXY_URL=
PROXY_POOL=
NO_PROXY=localhost,127.0.0.1

# === System Proxy Settings ===
# Windows Proxy: Ayarlar → Ağ ve İnternet → Proxy → Manuel proxy
# Adres: 127.0.0.1, Port: 1080
HTTP_PROXY=socks5://127.0.0.1:1080
HTTPS_PROXY=socks5://127.0.0.1:1080

# === Company Info ===
COMPANY_NAME=MercanDanismanlik 